{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "global": {
            "type": "object",
            "properties": {
                "podSecurityStandards": {
                    "type": "object",
                    "properties": {
                        "enforced": {
                            "type": "boolean"
                        }
                    }
                }
            }
        },
        "image": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "pullPolicy": {
                    "type": "string"
                },
                "registry": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "init": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "registry": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "initResources": {
            "type": "object",
            "properties": {
                "limits": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                },
                "requests": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "jiralert": {
            "type": "object",
            "properties": {
                "config": {
                    "type": "object",
                    "properties": {
                        "api_url": {
                            "type": "string"
                        },
                        "issue_type": {
                            "type": "string"
                        },
                        "reopen_state": {
                            "type": "string"
                        },
                        "resolution": {
                            "type": "string"
                        }
                    }
                },
                "receivers": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "add_group_labels": {
                                "type": "boolean"
                            },
                            "name": {
                                "type": "string"
                            },
                            "project": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "name": {
            "type": "string"
        },
        "networkPolicy": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                }
            }
        },
        "nodeSelector": {
            "type": "object"
        },
        "pod": {
            "type": "object",
            "properties": {
                "group": {
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "integer"
                        }
                    }
                },
                "user": {
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "integer"
                        }
                    }
                }
            }
        },
        "project": {
            "type": "object",
            "properties": {
                "branch": {
                    "type": "string"
                },
                "commit": {
                    "type": "string"
                }
            }
        },
        "psp": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                }
            }
        },
        "resources": {
            "type": "object",
            "properties": {
                "limits": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                },
                "requests": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "securityContext": {
            "type": "object",
            "properties": {
                "allowPrivilegeEscalation": {
                    "type": "boolean"
                },
                "capabilities": {
                    "type": "object",
                    "properties": {
                        "drop": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    }
                },
                "privileged": {
                    "type": "boolean"
                },
                "readOnlyRootFilesystem": {
                    "type": "boolean"
                },
                "runAsNonRoot": {
                    "type": "boolean"
                }
            }
        },
        "serviceType": {
            "type": "string"
        }
    }
}
